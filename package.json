{
    "name": "mlpipe-manager",
    "version": "0.1.0",
    "description": "NodeJS server backend to distribute & manage Keras/Tensorflow trainings.",
    "repository": {
        "type": "git",
        "url": "https://github.com/j-o-d-o/MLPipe-Manager"
    },
    "author": "Johannes Dobler",
    "license": "MIT",
    "scripts": {
        "start": "cross-env NODE_ENV=prod node build/src/server.js",
        "build": "npm run tslint && tsc",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "kill-port": "kill $(lsof -t -i:4000);",
        "test": "tsc && nyc --reporter=lcov --reporter=text-summary mocha",
        "test-single": "tsc && cross-env NODE_ENV=test mocha build/test/smokeTests/remoteTraining.test.js --no-opts --exit"
    },
    "_moduleDirectories": [
        "build/src",
        "build/test"
    ],
    "dependencies": {
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "aws-sdk": "^2.541.0",
        "body-parser": "^1.19.0",
        "bson": "^4.0.2",
        "cross-env": "^5.2.0",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.2.0",
        "flushwritable": "1.0.0",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.26",
        "mongodb": "^3.5.2",
        "mongoose": "^5.8.10",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "path": "^0.12.7",
        "raw-body": "^2.4.1",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "saslprep": "^1.0.3",
        "source-map-support": "^0.5.13",
        "ssh2": "^0.8.5",
        "sshpk": "1.16.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/bson": "^4.0.0",
        "@types/chai": "^4.2.3",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/mocha": "^5.2.7",
        "@types/mongodb": "^3.3.15",
        "@types/mongoose": "^5.5.43",
        "@types/morgan": "^1.7.37",
        "@types/multer": "^1.3.10",
        "@types/node": "^12.7.9",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/ssh2": "^0.5.39",
        "@types/sshpk": "^1.10.4",
        "@types/winston": "^2.4.4",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "mocha": "^6.2.1",
        "nodemon": "^1.19.3",
        "nyc": "^14.1.1",
        "tslint": "^5.20.0"
    }
}
